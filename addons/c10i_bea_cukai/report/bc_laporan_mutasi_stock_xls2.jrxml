	<queryString language="SQL">
		<![CDATA[SELECT item.id AS product_id
    , item.default_code AS product_code
    , itemtmpl.name AS itemname
    , uom.name AS uomname
    , sub.opening_qty AS init_qty
    , sub.incoming_qty AS balancein
    , sub.outgoing_qty AS balanceout
    , sub.adjustment_in_qty AS adjust_in
    , sub.adjustment_out_qty adjust_out
    , (sub.adjustment_in_qty - sub.adjustment_out_qty) AS adjustment
    , sub.closing_qty AS ending_qty
    , rc.name AS company_name
    FROM
        (SELECT
            pp.id as product_id,
            coalesce(opening_query.qty, 0.0) as opening_qty,
            coalesce(incoming_query.qty, 0.0) as incoming_qty,
            coalesce(outgoing_query.qty, 0.0) as outgoing_qty,
            coalesce(adjustment_in_query.qty, 0.0) as adjustment_in_qty,
            coalesce(adjustment_out_query.qty, 0.0) as adjustment_out_qty,
            coalesce(closing_query.qty, 0.0) as closing_qty
        FROM product_product pp
        LEFT JOIN (select product_id, sum(qty) as qty
            FROM ((SELECT product_id, product_qty AS qty
                 FROM stock_move sm
                 LEFT JOIN product_product pp ON pp.id = sm.product_id
                 LEFT JOIN product_template pt ON pt.id = pp.product_tmpl_id
                 LEFT JOIN product_category pc ON pc.id = pt.categ_id
                 LEFT JOIN product_type ptp ON ptp.id = pc.product_type
                 WHERE sm.location_id<>$P{location_id} AND sm.location_dest_id=$P{location_id}
                    AND sm.state='done' AND sm.company_id=$P{company_id} AND ptp.name =$P{product_type}
                    AND (date + INTERVAL '7 hours')::TIMESTAMP<($P{date_start}||' 00:00:01')::TIMESTAMP
                 )
                 UNION ALL
                 (SELECT product_id, -1*product_qty AS qty
                 FROM stock_move sm
                 LEFT JOIN product_product pp ON pp.id = sm.product_id
                 LEFT JOIN product_template pt ON pt.id = pp.product_tmpl_id
                 LEFT JOIN product_category pc ON pc.id = pt.categ_id
                 LEFT JOIN product_type ptp ON ptp.id = pc.product_type
                 WHERE sm.location_id=$P{location_id} AND sm.location_dest_id<>$P{location_id}
                    AND sm.state='done' AND sm.company_id=$P{company_id} AND ptp.name =$P{product_type}
                    AND (date + INTERVAL '7 hours')::TIMESTAMP<($P{date_start}||' 00:00:01')::TIMESTAMP
                )) sub
            group by product_id
            ) opening_query ON opening_query.product_id=pp.id
        LEFT JOIN (SELECT product_id, sum(qty) as qty
            FROM ((SELECT product_id, product_qty as qty
                 FROM stock_move sm
                 LEFT JOIN product_product pp ON pp.id = sm.product_id
                 LEFT JOIN product_template pt ON pt.id = pp.product_tmpl_id
                 LEFT JOIN product_category pc ON pc.id = pt.categ_id
                 LEFT JOIN product_type ptp ON ptp.id = pc.product_type
                 WHERE sm.location_id<>$P{location_id} AND sm.location_dest_id=$P{location_id}
                    AND sm.state='done' AND sm.company_id=$P{company_id} and ptp.name =$P{product_type}
                    AND (date + INTERVAL '7 hours')::TIMESTAMP<=($P{date_stop} ||' 23:59:59')::TIMESTAMP
                 )
                 UNION ALL
                 (SELECT product_id, -1*product_qty as qty
                 FROM stock_move sm
                 LEFT JOIN product_product pp ON pp.id = sm.product_id
                 LEFT JOIN product_template pt ON pt.id = pp.product_tmpl_id
                 LEFT JOIN product_category pc ON pc.id = pt.categ_id
                 LEFT JOIN product_type ptp ON ptp.id = pc.product_type
                 WHERE sm.location_id=$P{location_id} AND location_dest_id<>$P{location_id}
                    AND sm.state='done' AND sm.company_id=$P{company_id} and ptp.name =$P{product_type}
                    AND (date + INTERVAL '7 hours')::TIMESTAMP<=($P{date_stop} ||' 23:59:59')::TIMESTAMP
                )
                ) sub
            group by product_id
            ) closing_query ON closing_query.product_id=pp.id
        LEFT JOIN (SELECT
                product_id, sum(product_qty) as qty
            FROM stock_move sm
            LEFT JOIN product_product pp ON pp.id = sm.product_id
            LEFT JOIN product_template pt ON pt.id = pp.product_tmpl_id
            LEFT JOIN product_category pc ON pc.id = pt.categ_id
            LEFT JOIN product_type ptp ON ptp.id = pc.product_type
            WHERE sm.location_dest_id=$P{location_id}
		AND sm.location_id=any(select id from stock_location where usage!='inventory' AND id<>$P{location_id})
                AND sm.state='done' AND sm.company_id=$P{company_id} AND ptp.name =$P{product_type}
                AND (date + INTERVAL '7 hours')::TIMESTAMP between ($P{date_start}||' 00:00:01')::TIMESTAMP AND ($P{date_stop} ||' 23:59:59')::TIMESTAMP
            GROUP BY product_id) incoming_query ON incoming_query.product_id=pp.id
        LEFT JOIN (SELECT
                product_id, sum(product_qty) as qty
            FROM stock_move sm
            LEFT JOIN product_product pp ON pp.id = sm.product_id
            LEFT JOIN product_template pt ON pt.id = pp.product_tmpl_id
            LEFT JOIN product_category pc ON pc.id = pt.categ_id
            LEFT JOIN product_type ptp ON ptp.id = pc.product_type
            WHERE sm.location_id=$P{location_id}
                AND sm.location_dest_id=any(select id from stock_location where usage!='inventory' AND id<>$P{location_id})
                AND sm.state='done' and sm.company_id=$P{company_id} AND ptp.name =$P{product_type}
                AND (date + INTERVAL '7 hours')::TIMESTAMP between ($P{date_start}||' 00:00:01')::TIMESTAMP AND ($P{date_stop} ||' 23:59:59')::TIMESTAMP
            GROUP BY product_id) outgoing_query ON outgoing_query.product_id=pp.id
        LEFT JOIN (SELECT
                product_id, sum(product_qty) as qty
            FROM stock_move sm
            LEFT JOIN product_product pp ON pp.id = sm.product_id
            LEFT JOIN product_template pt ON pt.id = pp.product_tmpl_id
            LEFT JOIN product_category pc ON pc.id = pt.categ_id
            LEFT JOIN product_type ptp ON ptp.id = pc.product_type
            WHERE sm.location_dest_id=$P{location_id}
                AND sm.location_id=any(select id from stock_location where usage='inventory')
                AND sm.state='done' AND sm.company_id=$P{company_id} and ptp.name =$P{product_type}
                AND (date + INTERVAL '7 hours')::TIMESTAMP between ($P{date_start}||' 00:00:01')::TIMESTAMP and ($P{date_stop} ||' 23:59:59')::TIMESTAMP
            GROUP BY product_id) adjustment_in_query ON adjustment_in_query.product_id=pp.id
        LEFT JOIN (SELECT
                product_id, sum(product_qty) as qty
            FROM stock_move sm
            LEFT JOIN product_product pp ON pp.id = sm.product_id
            LEFT JOIN product_template pt ON pt.id = pp.product_tmpl_id
            LEFT JOIN product_category pc ON pc.id = pt.categ_id
            LEFT JOIN product_type ptp ON ptp.id = pc.product_type
            WHERE sm.location_id=$P{location_id}
                AND sm.location_dest_id=any(select id from stock_location where usage='inventory')
                AND sm.state='done' AND sm.company_id=$P{company_id} AND ptp.name =$P{product_type}
                AND (date + INTERVAL '7 hours')::TIMESTAMP between ($P{date_start}||' 00:00:01')::TIMESTAMP AND ($P{date_stop} ||' 23:59:59')::TIMESTAMP
            group by product_id) adjustment_out_query ON adjustment_out_query.product_id=pp.id
        WHERE
            (coalesce(opening_query.qty, 0.0)+coalesce(incoming_query.qty, 0.0)+coalesce(outgoing_query.qty, 0.0)+coalesce(adjustment_in_query.qty, 0.0)+coalesce(adjustment_out_query.qty,0.0)+coalesce(adjustment_out_query.qty,0.0))>0
            ) sub
        INNER JOIN product_product item ON item.id=sub.product_id and item.active=True
        INNER JOIN product_template itemtmpl ON itemtmpl.id=item.product_tmpl_id
        INNER JOIN product_uom uom ON uom.id=itemtmpl.uom_id
        INNER JOIN res_company rc ON rc.id=$P{company_id}
    WHERE itemtmpl.type='product']]>
	</queryString>
