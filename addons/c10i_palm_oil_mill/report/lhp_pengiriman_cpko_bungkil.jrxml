	<parameter name="mla_id" class="java.lang.Integer"/>
	<parameter name="mu_date" class="java.lang.String"/>
	<queryString language="SQL">
		<![CDATA[SELECT sol.id,
	so.client_order_ref AS pengiriman,
	sol.name,
	sol.order_id,
	so.date_order AS tanggal_kontrak,
	sol.product_uom_qty AS stok_akhir,
	sol.qty_delivered,
	(sol.product_uom_qty - sol.qty_delivered) AS sisa_kontrak,
	sol.qty_to_invoice,
	sol.qty_invoiced,
	sol.product_id
FROM sale_order_line sol
LEFT JOIN sale_order so ON so.id = sol.order_id
WHERE sol.state='sale' AND
	sol.invoice_status = 'to invoice' AND
	sol.qty_delivered < sol.product_uom_qty AND
	sol.qty_invoiced < sol.product_uom_qty AND
	(so.date_order + INTERVAL '7 hours')::TIMESTAMP<($P{mu_date}||' 00:00:00')::TIMESTAMP AND
	sol.qty_to_invoice < 0
ORDER BY sol.order_id ASC, sol.product_id]]>
	</queryString>
