	<queryString>
		<![CDATA[SELECT
	wsm."TIMBANG_NO" as no_tiket,
	wsm."TIMBANG_OUT_DATE" as tanggal,
	wsm."TIMBANG_RELASI" as partner_name,
	wsm."TIMBANG_KONTRAK" as contract_number,
	wsm."TIMBANG_DO" as do_number,
	bc.name as bc_number,
	bc.submission_date AS bc_date,
	wsm."TIMBANG_PRODUK" as product_name,
	wsm."TIMBANG_TIPETRANS" as trans_type,
    wpt.trans_type AS type_trans,
	wsm."KONTRAK_QUANTITY" as contract_qty,
	wsm."TIMBANG_OUT_USERLOG" as user,
	rt.name as transporter_name,
	wsm."TIMBANG_NOKENDARAAN" as vehicle_number,
	wsm."TIMBANG_SUPIR" as driver_name,
	to_char(wsm."TIMBANG_IN_TIMESTAMP" + interval '7 hours','HH24:MI') as in_time,
	to_char(wsm."TIMBANG_OUT_TIMESTAMP" + interval '7 hours','HH24:MI') as out_time,
	sol.price_unit AS harga_sale,
	wrml.netto * sol.price_unit AS harga_penyerahan_sale,
    pol.price_unit AS harga,
	wrml.netto * pol.price_unit AS harga_penyerahan,
	wrml.bruto,
	wrml.tarra,
	wrml.netto,
	wrml.partner_bruto,
	wrml.partner_tarra,
	wrml.partner_netto,
	(wrml.netto - wrml.partner_netto) as diff,
	wrml.internal_quality_ffa as ffa,
	wrml.internal_quality_kk as kk,
	wrml.internal_quality_ka as ka,
	sum(wrml.netto) over (partition by wsm."TIMBANG_DO") as delivered_qty,
	(wsm."KONTRAK_QUANTITY" - sum(wrml.netto) over (partition by wsm."TIMBANG_DO")) as remaining_qty
FROM weighbridge_recap_metro_line wrml
	INNER JOIN wizard_weighbridge_recap_metro wwrm ON wwrm.id = wrml.wizard_id
	LEFT JOIN weighbridge_scale_metro wsm On wsm.id = wrml.weighbridge_id
	LEFT JOIN weighbridge_partner wt On wt.id = wsm.wb_transporter_id
	LEFT JOIN res_partner rt On rt.id = wt.related_partner_id
	LEFT JOIN weighbridge_metro_picking_rel wprel ON wprel.weighbridge_id=wsm.id
	LEFT JOIN bea_cukai_picking_rel bcprel ON bcprel.stock_picking_id=wprel.picking_id
	LEFT JOIN bea_cukai bc ON bc.id=bcprel.bea_cukai_id
    LEFT JOIN weighbridge_contract wc ON wc.id=wsm.wb_contract_id
	LEFT JOIN sale_order_line sol ON sol.order_id=wc.related_sale_id
	LEFT JOIN purchase_order po ON po.bea_cukai_id=bc.id
	LEFT JOIN purchase_order_line pol ON pol.order_id=po.id
    LEFT JOIN weighbridge_picking_type wpt ON wpt.id=wsm.wb_picking_type_id
WHERE wrml.wizard_id = $P{wizard_metro_id}
UNION ALL
SELECT
	wsm."TIMBANG_NO" as no_tiket,
	wsm."TIMBANG_OUT_DATE" as tanggal,
	wsm."TIMBANG_RELASI" as partner_name,
	wsm."TIMBANG_SO" as contract_number,
	wsm."TIMBANG_DO" as do_number,
	bc.name as bc_number,
	bc.create_date AS bc_date,
	wsm."TIMBANG_PRODUK" as product_name,
	wsm."TIMBANG_TIPETRANS" as trans_type,
    wpt.trans_type AS type_trans,
	wsm."SO_QUANTITY" as contract_qty,
	wsm."TIMBANG_OUT_USERLOG" as user,
	rt.name as transporter_name,
	wsm."TIMBANG_NOKENDARAAN" as vehicle_number,
	wsm."TIMBANG_SUPIR" as driver_name,
	to_char(wsm."TIMBANG_IN_TIMESTAMP" + interval '7 hours','HH24:MI') as in_time,
	to_char(wsm."TIMBANG_OUT_TIMESTAMP" + interval '7 hours','HH24:MI') as out_time,
	sol.price_unit AS harga_sale,
	wrml.netto * sol.price_unit AS harga_penyerahan_sale,
    pol.price_unit AS harga,
	wrml.netto * pol.price_unit AS harga_penyerahan,
	wrml.bruto,
	wrml.tarra,
	wrml.netto,
	wrml.partner_bruto,
	wrml.partner_tarra,
	wrml.partner_netto,
	(wrml.netto - wrml.partner_netto) as diff,
	wrml.internal_quality_ffa as ffa,
	wrml.internal_quality_kk as kk,
	wrml.internal_quality_ka as ka,
	sum(wrml.netto) over (partition by wsm."TIMBANG_DO") as delivered_qty,
	(wsm."SO_QUANTITY" - sum(wrml.netto) over (partition by wsm."TIMBANG_DO")) as remaining_qty
FROM weighbridge_recap_sampit_line wrml
	INNER JOIN wizard_weighbridge_recap_sampit wwrm ON wwrm.id = wrml.wizard_id
	LEFT JOIN weighbridge_scale_sampit wsm On wsm.id = wrml.weighbridge_id
	LEFT JOIN weighbridge_partner wt On wt.id = wsm.wb_transporter_id
	LEFT JOIN res_partner rt On rt.id = wt.related_partner_id
	LEFT JOIN weighbridge_sampit_picking_rel wprel ON wprel.weighbridge_id=wsm.id
	LEFT JOIN bea_cukai_picking_rel bcprel ON bcprel.stock_picking_id=wprel.picking_id
	LEFT JOIN bea_cukai bc ON bc.id=bcprel.bea_cukai_id
    LEFT JOIN weighbridge_contract wc ON wc.id=wsm.wb_contract_id
	LEFT JOIN sale_order_line sol ON sol.order_id=wc.related_sale_id
	LEFT JOIN purchase_order po ON po.bea_cukai_id=bc.id
	LEFT JOIN purchase_order_line pol ON pol.order_id=po.id
    LEFT JOIN weighbridge_picking_type wpt ON wpt.id=wsm.wb_picking_type_id
WHERE wrml.wizard_id = $P{wizard_sampit_id}]]>
