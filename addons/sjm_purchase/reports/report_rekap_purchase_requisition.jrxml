	<queryString>
		<![CDATA[SELECT DISTINCT
	pr.name AS pr_no,
	pr.date AS pr_date,
	ou.name AS operating_unit,
	spt.name AS picking_type,
	pp.default_code AS product_code,
	pt.name AS product_name,
	prl.product_qty AS quantity,
	pu.name AS uom,
	prl.specifications AS spesifikasi,
	pt.last_purchase_price AS last_price,
	prl.note AS note,
	pr.state AS status,
	po.name AS realisasi_no_po
FROM
	purchase_request pr
	LEFT JOIN purchase_request_line prl ON prl.request_id=pr.id
	LEFT JOIN operating_unit ou ON ou.id=pr.operating_unit_id
	LEFT JOIN stock_picking_type spt ON spt.id=pr.picking_type_id
	LEFT JOIN product_product pp ON pp.id=prl.product_id
	LEFT JOIN product_template pt ON pt.id=pp.product_tmpl_id
	LEFT JOIN product_uom pu ON pu.id=prl.product_uom_id
	LEFT JOIN purchase_order_line pol ON pr.id=pol.request_id and pol.product_id=prl.product_id
	LEFT JOIN purchase_order po ON po.id=pol.order_id
WHERE
	pr.date::date between $P{date_start}::date and $P{date_stop}::date and $P!{operating_unit_query}
ORDER BY
	pr.name]]>
	</queryString>
